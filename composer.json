{
    "name": "jacksleight/laravel-raster",
    "autoload": {
        "psr-4": {
            "JackSleight\\LaravelRaster\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "JackSleight\\LaravelRaster\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "spatie/browsershot": "^4.3|^5.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.5",
        "phpstan/phpstan": "^1.12"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
